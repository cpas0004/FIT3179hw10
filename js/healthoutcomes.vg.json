{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
        "text": "Dispensed repiratory medications per state during the 2019/20 bushfires",
        "fontSize": 24     
    },
    "data": {
        "url": "https://raw.githubusercontent.com/cpas0004/FIT3179hw10/main/data/dispensed.csv",
        "format": {"type": "csv"}
    },
    "mark": "line",
    "encoding": {
      "x": {
        "field": "Date",
        "type": "temporal",
        "axis": {"title": "Date"}
      },
      "y": {
        "field": "Dispensed",
        "type": "quantitative",
        "axis": {"title": "Respiratory Prescriptions Dispensed"}
      },
      "color": {
        "field": "Category",
        "type": "nominal",
        "legend": {"title": "State"}
      }
    }
}